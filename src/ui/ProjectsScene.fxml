<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ProjectsSceneController">
    <children>
      <Label layoutX="323.0" layoutY="14.0" text="Vendor Management System" />
      <ButtonBar layoutX="268.0" layoutY="58.0" prefHeight="40.0" prefWidth="242.0">
        <buttons>
          <Button fx:id="projectsB" mnemonicParsing="false" text="Projects" />
            <Button mnemonicParsing="false" text="Items" />
            <Button mnemonicParsing="false" text="Vendors" />
        </buttons>
      </ButtonBar>
      <Button fx:id="closeB" layoutX="756.0" layoutY="16.0" mnemonicParsing="false" onAction="#handleCloseButtonAction" text="X">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <TableView fx:id="projectsTV" layoutX="113.0" layoutY="147.0" prefHeight="368.0" prefWidth="576.0">
        <columns>
          <TableColumn fx:id="projectTC" prefWidth="190.0" text="Project" />
          <TableColumn fx:id="itemTC" minWidth="0.0" prefWidth="195.0" text="Item" />
            <TableColumn fx:id="statusTC" prefWidth="99.0" text="Status" />
            <TableColumn prefWidth="91.0" text="Actions" />
        </columns>
      </TableView>
      <Button fx:id="newB" layoutX="659.0" layoutY="532.0" mnemonicParsing="false" onAction="#handleAddButtonAction" text="+">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
