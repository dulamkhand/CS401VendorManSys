<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EmployeeController">
   <children>
      <Label layoutX="323.0" layoutY="14.0" text="Vendor Management System" />
      <ButtonBar layoutX="268.0" layoutY="58.0" prefHeight="40.0" prefWidth="242.0">
        <buttons>
          <Button fx:id="projectsB" mnemonicParsing="false" onAction="#handleProjectsButtonAction" text="Projects" />
            <Button mnemonicParsing="false" text="Items" />
			<Button fx:id="ordersB" mnemonicParsing="false" onAction="#handleOrdersButtonAction" text="Orders" />
            <Button mnemonicParsing="false" text="Vendors" />
        </buttons>
      </ButtonBar>
      <Button fx:id="closeB" layoutX="756.0" layoutY="16.0" mnemonicParsing="false" onAction="#handleCloseButtonAction" text="X">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
